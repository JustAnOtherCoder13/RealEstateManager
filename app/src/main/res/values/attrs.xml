<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GlobalAttributeDeclare">
        <attr name="icon" format="reference"/>
        <attr name="title" format="string"/>
    </declare-styleable>

    <declare-styleable name="DetailInformationCustomView">
        <attr name="icon"/>
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="AddPropertyInformationCustomView">
        <attr name="icon"/>
        <attr name="title"/>
        <attr name="android:inputType"/>
        <attr name="android:singleLine"/>
        <attr name="android:lines"/>
    </declare-styleable>

    <declare-styleable name="CustomRangeSlider">
        <attr name="title"/>
        <attr name="android:stepSize"/>
    </declare-styleable>
    
</resources>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <CheckBox
        android:id="@+id/bottom_sheet_property_type_checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <TextView
        android:id="@+id/bottom_sheet_property_type_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Title"
        app:layout_constraintStart_toEndOf="@id/bottom_sheet_property_type_checkBox"
        app:layout_constraintTop_toTopOf="@id/bottom_sheet_property_type_checkBox"
        app:layout_constraintBottom_toBottomOf="@id/bottom_sheet_property_type_checkBox"
        android:textAppearance="@style/TextAppearance.RealEstateManagerStyles.Subtitle1"
        />


</androidx.constraintlayout.widget.ConstraintLayout>